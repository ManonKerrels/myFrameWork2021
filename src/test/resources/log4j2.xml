<?xml version="1.0" encoding="UTF-8" ?>
<Configuration status="WARN">

    <Appenders>

        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="[%t] %-5p | %-60c | %m (%F:%L)%n"/>
        </Console>

    </Appenders>

    <Loggers>

        <Root level="warn">
            <AppenderRef ref="stdout"/>
        </Root>

    </Loggers>

</Configuration>